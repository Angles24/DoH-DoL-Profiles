<Profile>
	<Name>80-85 Botanist</Name>
	<KillRadius>90</KillRadius>
	
	<Order>
		      
	<LogMessage Message="The Journey Begins! Remember to Check up on me!"/>
	
	<If Condition="ClassName != ClassJobType.Botanist">
		<ChangeClass Job="Botanist"/>
	</If>
				
    <If Condition="ClassName == ClassJobType.Botanist">
		<WaitTimer WaitTime="2"/>
		<AutoInventoryEquip/>
		<WaitTimer WaitTime="5"/>
				
		<!-- Level 85-87 -->
		<If Condition="Core.Player.ClassLevel &lt; 87">
			<LogMessage Message="Starting the Grind to Level 87."/>
			<Lisbeth Json="[{'Group':0, 'Item':36089, 'Amount':10, 'Enabled':true, 'Type':'Gather', 'Manual':26553, 'ConditionalLevel':87, 'ConditionalJob':'Botanist'},{'Group':0, 'Item':36205, 'Amount':15, 'Enabled':true, 'Type':'Gather', 'Manual':26553, 'ConditionalLevel':87, 'ConditionalJob':'Botanist'}]"/> <!-- Scarlet Moko Grass, Giant Popoto -->
		</If>
		<If Condition="Core.Player.ClassLevel == 87">
			<LogMessage Message="Congrats on Level 87, moving on."/>
			<WaitTimer WaitTime="2"/>
		<AutoInventoryEquip/>
		<WaitTimer WaitTime="5"/>
		</If>
		<!--End Level 87-->
		
		<!-- Level 87-88 -->
		<If Condition="Core.Player.ClassLevel &lt; 88">
			<LogMessage Message="Starting the Grind to Level 88."/>
			<Lisbeth Json="[{'Group':0, 'Item':36095, 'Amount':15, 'Enabled':true, 'Type':'Gather', 'Manual':26553, 'ConditionalLevel':88, 'ConditionalJob':'Botanist'}]"/> <!-- Elder Nutmeg Seeds -->
		</If>
		<If Condition="Core.Player.ClassLevel == 88">
			<LogMessage Message="Congrats on Level 88, moving on."/>
			<WaitTimer WaitTime="2"/>
		<AutoInventoryEquip/>
		<WaitTimer WaitTime="5"/>
		</If>
		<!--End Level 88-->
		
		<!-- Level 88-89 -->
		<If Condition="Core.Player.ClassLevel &lt; 89">
			<LogMessage Message="Starting the Grind to Level 89."/>
				<GetTo ZoneId="960" XYZ="-603.6246, 70.03349, 84.75305"/>
				<ExGather CordialType="Auto" CordialTime="Auto" While="Core.Player.ClassLevel &lt; 89">
					<GatherObjects>
						<GatherObject>Mature Tree</GatherObject>
					</GatherObjects>
					<ItemNames>
						<ItemName>Integral Log</ItemName>
					</ItemNames>
					<GatherSpots>
						<IndirectApproachGatherSpot ReturnToApproachLocation="false" ApproachLocation="-613.2001, 70.96008, 84.32868" nodeLocation="-614.3435, 72.52612, 84.21448" />
						<IndirectApproachGatherSpot ReturnToApproachLocation="false" ApproachLocation="-614.6254, 70.60738, 79.96552" nodeLocation="-615.198, 71.79371, 82.01721" />
						<IndirectApproachGatherSpot ReturnToApproachLocation="false" ApproachLocation="-760.3845, 59.96397, 36.20824" nodeLocation="-760.6202, 61.49334, 34.71832" />
						<IndirectApproachGatherSpot ReturnToApproachLocation="false" ApproachLocation="-762.4124, 60.40506, 36.00818" nodeLocation="-763.0071, 61.6693, 34.5165" />
						<IndirectApproachGatherSpot ReturnToApproachLocation="false" ApproachLocation="-712.595, 69.87288, -47.60133" nodeLocation="-713.5771, 70.89957, -48.86489" />
						<IndirectApproachGatherSpot ReturnToApproachLocation="false" ApproachLocation="-691.1491, 69.09055, -89.47324" nodeLocation="-691.3116, 70.68101, -91.12169" />
						<IndirectApproachGatherSpot ReturnToApproachLocation="false" ApproachLocation="-613.814, 70.5579, 80.79462" nodeLocation="-615.198, 71.79371, 82.01721" />
						<IndirectApproachGatherSpot ReturnToApproachLocation="false" ApproachLocation="-613.6422, 71.28991, 84.10622" nodeLocation="-614.3435, 72.52612, 84.21448" />
					</GatherSpots>			
					<HotSpots>
						<HotSpots Radius="45" XYZ="-603.6246, 70.03349, 84.75305" />
						<HotSpots Radius="45" XYZ="-752.2568, 59.46293, 33.39579"/>
						<HotSpots Radius="45" XYZ="-694.3452, 69.7803, -67.01098"/>
					</HotSpots>
				</ExGather>
			<!--<Lisbeth Json="[{'Group':0, 'Item':36194, 'Amount':15, 'Enabled':true, 'Type':'Gather', 'Manual':26553, 'ConditionalLevel':89, 'ConditionalJob':'Botanist'}]"/>--> <!-- Integral Log -->
		</If>
		<If Condition="Core.Player.ClassLevel == 89">
			<LogMessage Message="Congrats on Level 89, moving on."/>
			<WaitTimer WaitTime="2"/>
		<AutoInventoryEquip/>
		<WaitTimer WaitTime="5"/>
		</If>
		<!--End Level 89-->
		
		<!-- Level 89-90 -->
		<If Condition="Core.Player.ClassLevel &lt; 90">
			<LogMessage Message="Starting the Grind to Level 90."/>
			<GetTo ZoneId="960" XYZ="-352.734, 86.8394, 274.3643"/>
			<ExGather CordialType="Auto" CordialTime="Auto" While="Core.Player.ClassLevel &lt; 90">
				<GatherObjects>
					<GatherObject>Lush Vegetation Patch</GatherObject>
				</GatherObjects>
				<ItemNames>
					<ItemName>AR-Caean Cotton</ItemName>
				</ItemNames>
				<HotSpots>
					<HotSpots XYZ="-352.734, 86.8394, 274.3643" Radius="20" />
					<HotSpots XYZ="-247.7067, 80.52891, 347.7692" Radius="20" />
					<HotSpots XYZ="-416.0974, 88.50111, 382.3748" Radius="20" />
				</HotSpots>
			</ExGather>
			<!--<Lisbeth Json="[{'Group':0, 'Item':36206, 'Amount':15, 'Enabled':true, 'Type':'Gather', 'Manual':26553, 'ConditionalLevel':90, 'ConditionalJob':'Botanist'}]"/>--> <!-- AR-Caean Cotton -->
		</If>
		<If Condition="Core.Player.ClassLevel == 90">
			<LogMessage Message="Congrats on Level 90, moving on."/>
			<WaitTimer WaitTime="2"/>
		<AutoInventoryEquip/>
		<WaitTimer WaitTime="5"/>
		</If>
		<!--End Level 90-->
		
	</If>
	
	<LLoadProfile Path="../Start.xml"/>
	
	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
		<CodeChunk Name="MINWeapon">
			<![CDATA[
				var item = InventoryManager.FilledInventoryAndArmory.FirstOrDefault(i => i.Item.EquipmentCatagory == ItemUiCategory.Miners_Primary_Tool);
				BagSlot EquipSlot = InventoryManager.GetBagByInventoryBagId(InventoryBagId.EquippedItems)[EquipmentSlot.MainHand];
				if (item != null)
				{
					item.Move(EquipSlot);
				}
		]]>
		</CodeChunk>
		<CodeChunk Name="MINWeapon2">
			<![CDATA[
				var item = InventoryManager.FilledInventoryAndArmory.FirstOrDefault(i => i.Item.EquipmentCatagory == ItemUiCategory.Miners_Secondary_Tool);
				BagSlot EquipSlot = InventoryManager.GetBagByInventoryBagId(InventoryBagId.EquippedItems)[EquipmentSlot.OffHand];
				if (item != null)
				{
					item.Move(EquipSlot);
				}
		]]>
		</CodeChunk>
		<CodeChunk Name="Sneak">
			<![CDATA[ 
				if(!Core.Player.HasAura(47)) 
				{ 
					ff14bot.Managers.ActionManager.DoAction("Sneak",Core.Player); 
					await Coroutine.Wait(5000, () => Core.Player.HasAura(47)); 
				} 
		]]>
		</CodeChunk>
		<CodeChunk Name="Enable_Flight">
      <![CDATA[
        foreach (var plugin in PluginManager.Plugins.Where(p => p.Plugin.Name == "Enable Flight" || p.Plugin.Name == "启用飞行"))
        {
          try
          {
            if (plugin.Enabled)  {  break;  }  else  {  plugin.Enabled = true;  }
          }
          catch (Exception ex) {  }
        }
      ]]>
    </CodeChunk>
	<CodeChunk Name="Disable_Flight">
      <![CDATA[
        foreach (var plugin in PluginManager.Plugins.Where(p => p.Plugin.Name == "Enable Flight" || p.Plugin.Name == "启用飞行"))
        {
          try
          {
            if (!plugin.Enabled)  {  break;  }  else  {  plugin.Enabled = false;  }
          }
          catch (Exception ex) {  }
        }
      ]]>
    </CodeChunk>
	</CodeChunks>
</Profile>
